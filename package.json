{
  "name": "library-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "npm run test && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "lint:styles": "stylelint src/scss/**/**.{css,scss} --config ./.stylelintrc.json",
    "lint:scripts": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --config eslint.config.mjs",
    "lint": "npm run lint:styles && npm run lint:scripts",
    "types": "tsc --noEmit --project tsconfig.json",
    "lint:fix": "stylelint src/scss/**/**.{css,scss} --config ./.stylelintrc.json --fix && eslint --cache --cache-location ./node_modules/.cache/.eslintcache --config eslint.config.mjs --fix",
    "editorconfig": "editorconfig-cli -x \"vendor/\"",
    "format": "prettier --write ./**/**.{js,ts} && prettier --write ./**/**.pug",
    "test": "npm run editorconfig && npm run lint && npm run types",
    "lint-staged": "lint-staged"
  },
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "src/**/*.html",
    "src/**/*.pug",
    "src/js/**/*.js",
    "src/js/**/*.ts",
    "src/img/**/*.svg",
    "src/scss/**/*.{sass,scss}"
  ],
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.19.0",
    "@htmlacademy/editorconfig-cli": "^3.0.0",
    "@prettier/plugin-pug": "^3.2.0",
    "@stylistic/stylelint-plugin": "^2.1.3",
    "@types/node": "^22.13.1",
    "@types/pug": "^2.0.10",
    "@types/scroll-lock": "2.1.3",
    "@types/vanillajs-datepicker": "1.3.5",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "~9.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-promise": "^7.2.1",
    "glob-exec": "^0.1.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nodejs-logger-n": "^1.0.3",
    "postcss": "^8.5.1",
    "postcss-load-config": "^5.1.0",
    "postcss-nesting": "^12.1.5",
    "postcss-sorting": "^8.0.2",
    "prettier": "^3.4.2",
    "pug": "^3.0.3",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "sass": "^1.83.4",
    "sass-migrator": "^2.3.1",
    "stylelint": "^16.14.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.11.0",
    "terser": "^5.37.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^7.18.0",
    "vite": "^5.4.14",
    "vite-mpa": "^1.1.4",
    "vite-plugin-chunk-split": "^0.5.0",
    "vite-plugin-pug": "^0.3.2",
    "vite-plugin-stylelint": "^5.3.1",
    "vite-plugin-svg-spriter": "1.0.0"
  },
  "browserslist": [
    "last 3 versions",
    "not ie <= 11",
    "Firefox ESR"
  ],
  "dependencies": {}
}
